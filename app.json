{
  "expo": {
    "name": "bookease",
    "description": "The painless, no-frills textbook exchange!",
    "slug": "bookease",
    "privacy": "unlisted",
    "sdkVersion": "23.0.0",
    "version": "0.1.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "notification": {
      "icon": "./assets/icons/notification-icon.png",
      "color": "#000000"
    },
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.bookeaseapp",
      "permissions": ["CAMERA", "ACCESS_FINE_LOCATION", "READ_CONTACTS"]
    }
  }
}
